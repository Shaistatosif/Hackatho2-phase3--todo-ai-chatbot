{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "regions": ["iad1"],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://shaista-tosif-todo-ai-backend.hf.space",
    "AUTH_SECRET": "this-is-a-placeholder-secret-key-32chars",
    "NEXT_PUBLIC_APP_URL": "https://frontend-tau-three-31.vercel.app"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_API_URL": "https://shaista-tosif-todo-ai-backend.hf.space",
      "AUTH_SECRET": "this-is-a-placeholder-secret-key-32chars"
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}
